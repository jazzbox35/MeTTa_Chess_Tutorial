(= (SpeedFunction) (1st Plan))
(= (CostFunction) (2nd Plan))
(= (EfficiencyFunction) (3rd Plan))

(= (DevisePlan $strategy)
   (case $strategy
          (
          (Speed      (SpeedFunction))      
          (LowCost    (CostFunction))       
          (Efficiency (EfficiencyFunction)) 
          ($_         (Invalid))               ; OTHERWISE
          )))
!(DevisePlan (superpose (Speed LowCost Efficiency)))